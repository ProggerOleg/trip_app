etForecast(trip.city, transformDateFormat(trip.date1), transformDateFormat(trip.date2)).then(item => {\n        setWeekWeather(item.days)\n      })\n    }\n  }, [trip])\n\n  const addTrips = (image, city, date1, date2) => {\n    const newTrip = { image, city, date1, date2 };\n    setTrips((prevData) => sortTrips([...prevData, newTrip]));\n  };\n\n  useEffect(() => {\n    if (trips.length > 0) {\n      localStorage.setItem(\"cardData\", JSON.stringify(trips));\n    }\n  }, [trips]);\n\n  // Сортируем поездки по начальной дате\n  const sortTrips = (arr) => [...arr].sort((a, b) => {\n    const dateA = new Date(a.date1.split(\".\").reverse().join(\"-\"));\n    const dateB = new Date(b.date1.split(\".\").reverse().join(\"-\"));\n    return dateA - dateB;\n  });\n\n  return (\n    <div className=\"App\">\n      <Trip weekWeather={weekWeather} trips={trips} setTrip={setTrip} setModal={setModal} />\n      <Weather weather={weather} trip={trip} />\n      <Modal modal={modal} addTrips={addTrips} setModal={setModal} />\n    </div>\n  );\n}\n\nexport default App;",[],"/home/oleg/test_zadaniye/Reenbit/weather-app/src/services/WeatherService.js",[],[],"/home/oleg/test_zadaniye/Reenbit/weather-app/src/hooks/http.hook.js",[],[],"/home/oleg/test_zadaniye/Reenbit/weather-app/src/components/trip/Trip.js",[],[],"/home/oleg/test_zadaniye/Reenbit/weather-app/src/components/trip/Weather.js",[],[],"/home/oleg/test_zadaniye/Reenbit/weather-app/src/components/cards/Card.js",[],[],"/home/oleg/test_zadaniye/Reenbit/weather-app/src/components/modal/Modal.js",[],[],"/home/oleg/test_zadaniye/Reenbit/weather-app/src/assets/Images.js",[],[],"/home/oleg/test_zadaniye/Reenbit/weather-app/src/components/cards/Day.js",[],[],"/home/oleg/test_zadaniye/Reenbit/weather-app/src/components/Carousel/Carousel.js",["66","67","68"],[],"import { useState } from 'react';\nimport { GrFormPrevious, GrFormNext } from 'react-icons/gr'\nimport { AiOutlinePlus } from \"react-icons/ai\";\nimport './Carousel.css';\n\n\nco